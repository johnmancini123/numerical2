function w = poissonninepoint(a, b, h, n, f)
    %construct coefficient matrix
    A = zeros(n, n);
    
    v1 = zeros(1 ,n) + (-10/3);
    A = diag(v1);
    
    for i = 1: n-1
        if mod(i, 3) ~= 0
            A(i, i+1) = 2/3; 
            A(i+1, i) = 2/3;
        end
    end
    
    w = 1;
    v1 = zeros(1, n-3) + 2/3;
    temp = diag(v1, 3);
    A = A + temp;
    temp = diag(v1, -3);
    A = A + temp;
    
    for i = 1: n-3
        if mod(i, 3) ~= 0
            A(i+1, i+3) = 1/6; 
            A(i+3, i+1) = 1/6;
        end
    end
    
    for i = 1:n-4
       if mod(i,3) ~= 0
          A(i, i+4) = 1/6;
          A(i+4, i) = 1/6;
       end
    end
    
    disp(A);
    v1 = zeros(1, n) + 2/3;
    bTemp = diag(v1);
    v2 = zeros(1,n-1) +1/12;
    temp2 = diag(v2, 1);
    bTemp = bTemp + temp2;
    temp2 = diag(v2, -1);
    bTemp = bTemp + temp2;
    disp(bTemp);
    
    
    x = a:h:b;
    y = a:h:b;
    [bx, by] = meshgrid(x, y);
    m = 19;
    flatbx = reshape(by, [(m+1)*(m+1), 1]);
    flatby = reshape(bx, [(m+1)*(m+1), 1]);
    
    
    b = zeros(n, 1);
    for i =1:n
        b(i) = h^2*(1/12*f(flatbx(i+1), flatby(i)) + ...
            1/12*f(flatbx(i), flatby(i+1)) + ...
            2/3*f(flatbx(i+1), flatby(i+1)) +...
            1/12 * f(flatbx(i+2), flatby(i+2))
end